<% if current_user %>
  <div class="container"><h3>Welcome <%= @current_user.name %></h3></div>
  <div class="container">
    <h3>Current Users</h3>
    <% @users.each do |user| %>
      <p>Name: <%= user.name%>&nbsp;&nbsp;-&nbsp;&nbsp;Email: <%= user.email %>&nbsp;&nbsp;-&nbsp;&nbsp;Joined: <%= user.created_at.strftime("%c")%><hr>
    <% end %>
  </div>
<% else %>
  <div class="container"><h3>Welcome! Please sign in or up to continue....</h3></div>
<% end %>